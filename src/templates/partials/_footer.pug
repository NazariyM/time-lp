//begin footer
+b.FOOTER.footer
	+e.container.container
		+e.P.item © 2018 «Время поручений»

		+b.socials.footer__socials.footer__item
			+e.SPAN Мы в соц. сетях:
			-
				socList = [
				['vk', 'https://vk.com/vremyaporucheniy'],
				['inst', 'https://www.instagram.com/vremyaporucheniy/'],
				['fb', 'https://www.facebook.com/vremyaporucheniy/']]
			for socItem in socList
				+e.A(href=`${socItem[1]}` target='_blank'): +i(`${socItem[0]}`)

	+e.container.container
		+e.A.link.link._blue._has-icon(href='data/offer-agreement.pdf' target='_blank')
			+i('pdf-logo')
			span Договор-оферты
		+e.A.link.link._blue._has-icon(href='data/confidentiality-policy.pdf' target='_blank')
			+i('pdf-logo')
			span Политика конфиденциальности
		+e.A.link.link._blue._has-icon(href='data/services-agreement.pdf' target='_blank')
			+i('pdf-logo')
			span Договор для юр. лиц

//end footer
